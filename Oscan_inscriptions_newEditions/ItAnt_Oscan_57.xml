                        <tei:rdg corresp="#Osc_57_l_1_w_3">fḷ[akís]
                            <tei:ref target="http://zotero.org/groups/2552746/items/IMWMSAVE"><tei:bibl>Rix, ST 2002</tei:bibl></tei:ref>                          
                        </tei:rdg>                        
                        <tei:rdg corresp="#Osc_57_l_1_w_3">fa[
                            <tei:ref target="http://zotero.org/groups/2552746/items/3NP7P7IK"><tei:bibl>De Benedittis 1978</tei:bibl></tei:ref>

                            <!-- Leida;Osc_407_l_1_w_3;[l](úvkis) -->
                            <tei:name type="praenomen" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_407_l_1_w_3" ref="#p1"><tei:expan><tei:abbr><tei:supplied reason="lost">l</tei:supplied></tei:abbr><tei:ex>úvkis</tei:ex></tei:expan></tei:name>
                            <!-- Leida;Osc_407_l_1_w_4;[st]ạí(is) -->
                            <tei:name type="gentilicium" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_407_l_1_w_4" ref="#p1"><tei:expan><tei:abbr><tei:supplied reason="lost">st</tei:supplied><tei:unclear>a</tei:unclear>í</tei:abbr><tei:ex>is</tei:ex></tei:expan></tei:name>                           
                            <!-- Leida;Osc_407_l_1_w_5;ú(vieís) -->
                            <tei:name type="patronymic" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_407_l_1_w_5" ref="#p1"><tei:expan><tei:abbr>ú</tei:abbr><tei:ex>vieís</tei:ex></tei:expan></tei:name>
                            <!-- Leida;Osc_407_l_1_w_6;m(i)[t](eleís) -->
                            <tei:name type="avonymic" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_407_l_1_w_6" ref="#p1"><tei:expan><tei:abbr>m</tei:abbr><tei:ex>i</tei:ex><tei:abbr><tei:supplied reason="lost">t</tei:supplied></tei:abbr><tei:ex>eleís</tei:ex></tei:expan></tei:name>
                    </tei:ab>

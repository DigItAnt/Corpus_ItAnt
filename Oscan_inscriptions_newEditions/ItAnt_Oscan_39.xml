                    <tei:w xml:lang="osc-Ital-x-oscetr" xml:id="Osc_39_l_1_w_2">praí</tei:w>  
                    <!-- vacat -->
                    <tei:space unit="character" quantity="1"/>
                    <!-- Leida;Osc_39_l_1_w_3;kel[ -->
                    <tei:w xml:lang="osc-Ital-x-oscetr" xml:id="Osc_39_l_1_w_3">kel<tei:gap reason="lost" extent="unknown" unit="character"/></tei:w>

        <tei:body>    
            <tei:div type="edition" subtype="interpretative" xml:space="preserve">
                <tei:div type="textpart" n="fragment_A" style="text-direction:r-to-l" rend="ductus:sinistrorse" xml:id="fragment_A">
                    <tei:ab>   

                    <!-- Leida;Osc_39_l_1_w_5;kú[ -->
                    <tei:w xml:lang="osc-Ital-x-oscetr" xml:id="Osc_39_l_1_w_5">kú<tei:gap reason="lost" extent="unknown" unit="character"/></tei:w>

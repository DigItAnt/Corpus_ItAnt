    <tei:text>
        <tei:body>             
            <tei:div type="edition" subtype="interpretative" xml:space="preserve">
                <tei:div type="textpart" n="face_a" style="text-direction:r-to-l" rend="ductus:sinistrorse">
                    <tei:ab>   
                        <tei:lb n="1" xml:id="Osc_305_l_1"/>
                            <!-- Leida;Osc_305_l_1_w_1;siviiú -->
                            <tei:name type="gentilicium" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_305_l_1_w_1" ref="#p1">siviiú</tei:name>
                        <!-- Leida;Osc_305_l_1_w_2;magiú -->
                            <tei:name type="gamonymic" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_305_l_1_w_2" ref="#p1">magiú</tei:name>
                        <tei:space/>
                        <tei:lb n="2" xml:id="Osc_305_l_2"/>
                            <!-- Leida;Osc_305_l_1_w_1;mefit(eí) -->
                            <tei:name type="theonym" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_305_l_2_w_1"><tei:expan><tei:abbr>mefit</tei:abbr><tei:ex>eí</tei:ex></tei:expan></tei:name>
                        <tei:space/>
                        <tei:space unit="line"/>
                    </tei:ab>
                </tei:div>

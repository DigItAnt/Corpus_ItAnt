                             tiis
                             <tei:pc unit="word">:</tei:pc>
                             <!-- Leida;Osc_35_l_2_w_1;m(ara)h(eís) -->
                             <tei:name type="patronymic" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_35_l_2_w_2" ref="#p1"><tei:expan><tei:abbr>m</tei:abbr><tei:ex>ara</tei:ex><tei:abbr>h</tei:abbr><tei:ex>eís</tei:ex></tei:expan></tei:name>

                            <tei:name type="gentilicium" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_489_l_1_w_1" ref="#p1"><tei:expan><tei:abbr><tei:supplied reason="lost">ahv</tei:supplied>ud</tei:abbr><tei:ex>iis</tei:ex></tei:expan></tei:name>
                            <!-- Leida;Osc_l_1_w_2;me(tieís) -->
                            <tei:name type="patronymic" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_489_l_1_w_2" ref="#p1"><tei:expan><tei:abbr>me</tei:abbr><tei:ex>tieís</tei:ex></tei:expan></tei:name>
                    </tei:ab>

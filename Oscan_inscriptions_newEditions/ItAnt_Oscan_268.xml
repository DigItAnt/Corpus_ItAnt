                            <tei:w xml:lang="osc-Ital-x-oscetr" xml:id="Osc_268_l_2_w_1"><tei:gap reason="lost" extent="unknown" unit="character"/>s</tei:w>
                        <tei:pc unit="word">.</tei:pc>
<!-- Leida;Osc_268_l_2_w_2;deded -->                        
                            <tei:w xml:lang="osc-Ital-x-oscetr" xml:id="Osc_268_l_2_w_2">deded</tei:w>

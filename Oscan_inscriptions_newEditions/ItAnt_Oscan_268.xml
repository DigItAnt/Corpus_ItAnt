<!-- Leida;Osc_268_l_2_w_3;aragetú[d -->                        
                            <tei:w xml:lang="osc-Ital-x-oscetr" xml:id="Osc_268_l_2_w_3"><tei:unclear>a</tei:unclear>raget<tei:unclear>ú</tei:unclear><tei:supplied reason="lost">d</tei:supplied></tei:w>
                        <tei:supplied reason="lost"><tei:pc unit="word">.</tei:pc></tei:supplied>                        

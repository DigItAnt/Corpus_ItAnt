                                    <tei:pc unit="word">.</tei:pc>
<!-- Leida;Osc_268_l_1_w_2;plífrík[s -->                        
                                <tei:w xml:lang="osc-Ital-x-oscetr" xml:id="Osc_268_l_1_w_2"><tei:unclear>p</tei:unclear>lí<tei:unclear reason="illegible">frik</tei:unclear><tei:supplied reason="lost">s</tei:supplied></tei:w>
                        <tei:supplied reason="lost"><tei:pc unit="word">.</tei:pc></tei:supplied>
<!-- gap -->                       

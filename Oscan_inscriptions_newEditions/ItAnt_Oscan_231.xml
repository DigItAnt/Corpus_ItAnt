                        <!--  <tei:rdg corresp="Osc_231_l_3_w_2">st<tei:supplied reason="lost" evidence="previouseditor">a</tei:supplied>
                            <tei:ref target="http://zotero.org/groups/2552746/items/ZK74ZFDC"><tei:bibl>La Regina 2008 [2011]</tei:bibl></tei:ref>
                        </tei:rdg> -->
                        <tei:rdg corresp="Osc_231_l_3_w_2">stạị́[t
                            <tei:ref target="http://zotero.org/groups/2552746/items/IKUTPHDC"><tei:bibl>Crawford, ImIt</tei:bibl></tei:ref>
                        </tei:rdg>
                        <tei:rdg corresp="Osc_231_l_3_w_2">stai[t
                            <tei:ref target="http://zotero.org/groups/2552746/items/IMWMSAVE"><tei:bibl>Rix, ST</tei:bibl></tei:ref>

                    <tei:ab>   
                        <tei:lb n="1" xml:id="Osc_56_l_1"/>
                            <!-- Leida;Osc_56_l_1_w_1;tía vel aít vel taí -->
                            <tei:name type="praenomen" xml:lang="osc-Ital-x-oscetr" xml:id="Osc_56_l_1_w_1" ref="#p1">
                                <tei:hi rend="ligature">
                                    <tei:expan>
                                        <tei:abbr>tía      
                                        </tei:abbr>

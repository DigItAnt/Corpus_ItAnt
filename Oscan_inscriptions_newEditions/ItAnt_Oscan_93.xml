        <tei:body>             
            <tei:div type="edition" subtype="interpretative" xml:space="preserve">
                <tei:div type="textpart" n="face_a" style="text-direction:r-to-l" rend="ductus:sinistrorse">
                    <tei:ab>   
                        <tei:lb n="1" xml:id="Osc_93_l_1"/>
                            <!-- Leida;Osc_93_l_1_w_1;v+ [ -->
                            <tei:w xml:lang="osc-Ital-x-oscetr" xml:id="Osc_93_l_1_w_1">v<tei:gap reason="illegible" quantity="1" unit="character"/></tei:w>
